{
    "name": "mvanduijker/laravel-transactional-model-events",
    "description": "Add eloquent model events fired after a transaction is committed or rolled back",
    "keywords": [
        "laravel",
        "eloquent",
        "transaction",
        "database",
        "events",
        "laravel-transactional-model-events"
    ],
    "homepage": "https://github.com/mvanduijker/laravel-transactional-model-events",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van Duijker",
            "email": "mark@duyker.nl",
            "homepage": "https://github.com/mvanduijker",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "~6.0,!=6.9.0|~7.0|~8.0|~9.0|~10.0|~11.0|^12.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "~4.0|~5.0|~6.0|~7.0|~8.0|~9.0|^10.0",
        "phpunit/phpunit": "^9.3|^10.0|^11.5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MVanDuijker\\TransactionalModelEvents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MVanDuijker\\TransactionalModelEvents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
